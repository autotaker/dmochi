let zip : [int, int] -> int =
    fun x y ->
        if x = 0 then
            if y = 0 then 0
            else Fail
        else if y = 0 then
            Fail
        else
            let r = zip (x - 1) (y - 1) in
            1 + r;;

let map : [int] -> int =
    fun x -> if x = 0 then 0 else let r = map (x - 1) in 1 + r;;

let n = * in
let r = zip n n in
let r = map r in
if r >= n then
    if r <= n then
        0
    else Fail
else Fail;;

